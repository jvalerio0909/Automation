package PageObjects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;

import Utilities.Utilities;

public class EmployeesListPOM extends Utilities{
	
	public WebElement administrativeTab() {
		return driver.findElement(By.linkText("Administrative Users"));
	}
	
	public WebElement createUserLnk() {
		return driver.findElement(By.linkText("Create a new Admin user"));
	}

}
